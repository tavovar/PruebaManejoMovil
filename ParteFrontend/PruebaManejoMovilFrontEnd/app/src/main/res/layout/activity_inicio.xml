<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="moviles.cr.tec.pruebamanejomovilfrontend.Inicio">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ibTestTeorico"
        android:src="@drawable/ic_launcher"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="70dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ibTestPractico"
        android:src="@drawable/ic_launcher"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="210dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ibManual"
        android:src="@drawable/ic_launcher"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="70dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ibLugar"
        android:src="@drawable/ic_launcher"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="210dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ibResultados"
        android:src="@drawable/ic_launcher"
        android:layout_marginTop="300dp"
        android:layout_marginLeft="70dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ibSugerencia"
        android:src="@drawable/ic_launcher"
        android:layout_marginTop="300dp"
        android:layout_marginLeft="210dp" />

</RelativeLayout>
